{
  "name": "lotto-multichain-cli",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build" : "yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package interfaces --input ./metadata",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package ./interfaces --output .src/interfaces --endpoint wss://rps.astar.network"
  },
  "author": "GuiGou",
  "description": "",
  "dependencies": {
    "@polkadot/api": "^13.2.1",
    "@polkadot/api-contract": "^13.2.1",
    "@polkadot/typegen": "^13.2.1",
    "@phala/sdk": "^0.6.0-beta.19",
    "node-fetch": "^3.3.2",
    "yargs": "^17.0.22",
    "@types/yargs": "^17.0.22",
    "tsx": "^4.7.2",
    "ethers": "^6.13.4"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.11",
    "typescript": "^5.4.4",
    "ts-node": "^10.9.2"
  }
}
